RewriteEngine On

# /logout or /logout/ â†’ /api/logout.php (internal rewrite)
RewriteRule ^logout/?$ data/api/logout.php [L]

# Redirect /file/variable to /file.php?var=variable
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([a-zA-Z0-9_]+)/(.*)$ $1.php?var=$2 [L,QSA]
